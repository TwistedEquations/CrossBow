<?xml version="1.0" encoding="UTF-8"?>
<project name="com.android.com.crossbow.volley-rules" default="help">

  <!-- A rule to generate the JAR for inclusion in an Android
       application. Output file will be bin/com.android.com.requestQueue.volley.jar -->
  <target name="jar" depends="-compile">
    <jar destfile="bin/com.android.com.crossbow.volley.jar"
         basedir="bin/classes" />
  </target>
</project>
